  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "outDir": "dist",
    "typeRoots": ["typings", "node_modules/@types"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react-native",
    "forceConsistentCasingInFileNames": true,
  },
  "exclude": ["setupJest.ts"]
}
